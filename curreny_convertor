choice = input("enter your choice:")
if choice == "b" or choice == "B":
    def func1():
        print("this program convert Pound Sterling to US dollars ")
        print()

        pounds = eval(input("enter the amount in pounds"))
        dollars = convert_to_dollars(pounds)
        print("that is", dollars, "dollars")
    convert_to_dollars = lambda pounds: pounds * 1.27
    func1()


elif choice == "a" or choice == "A":
    def func2():
        print("this program convert Pound Sterling to Indian Rupee ")
        print()

        pounds = eval(input("enter the amount in pounds"))
        rupee = convert_to_rupee(pounds)
        print("that is", rupee, "rupee")


    convert_to_rupee = lambda pounds: pounds * 1.27
    func2()

